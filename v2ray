#系统升级为testing，国内安装v2ray
echo "
#deb http://http.us.debian.org/debian/ testing non-free contrib main
deb http://mirrors.ustc.edu.cn/debian/ testing non-free contrib main
"     >>     /etc/apt/sources.list
apt   update
apt   full-upgrade  -y
apt   autoremove    -y

apt   install       -y     golang-v2ray-core





